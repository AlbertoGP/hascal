local use sdl2

function main(): int {
    var window = sdl2_new_window("Hello World",100,100)
    var event : sdl2_event
    var quit : bool = false

    while(not quit){
        while(sdl2_poll_event(event)){
            // SDL_QUIT = 256
            if(event.type == 256){
                quit = true
            }
        }
    }
    sdl2_destroy_window(window)
    sdl2_quit()
    return sdl2_exit()
}